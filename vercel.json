{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.workpaytools.com"
        }
      ],
      "destination": "https://workpaytools.com/$1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "workpaytools.com"
        }
      ],
      "missing": [
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "https"
        }
      ],
      "destination": "https://workpaytools.com/$1",
      "permanent": true
    }
  ]
}